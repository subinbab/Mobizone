@model DomainLayer.ProductModel.Master.MasterTable

@{
    ViewData["Title"] = "MasterEdit";
}

<div class="row">
    <div class="col-md-4">
        
        <form asp-action="MasterEdit" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <input asp-for="id" type="hidden"/>
                <div class="form-group col-6">
                <input asp-for="masterData" type="text"/>
                <span asp-validation-for="masterData" class="text-danger"></span>
                </div>
                <input asp-for="parantId" type="hidden"/>
                <input type="submit" value="Edit" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
