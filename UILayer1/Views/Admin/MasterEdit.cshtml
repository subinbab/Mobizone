@model DomainLayer.ProductModel.Master.MasterTable

@{
    ViewData["Title"] = "Mobizone(Admin) MasterEdit";
    Layout = "~/Views/Admin/_Layout.cshtml";
}


<div class="row">
    <div class="col-md-4">

        <form asp-action="MasterEdit" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <input asp-for="id" type="hidden" />
                <div class="form-group col-6">
                    <input asp-for="masterData" type="text" />
                    <span asp-validation-for="masterData" class="text-danger"></span>
                </div>
                <input asp-for="parantId" type="hidden" />
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<style>
    .navbar-brand {
        display: inline-block;
        padding-top: 0.3125rem;
        padding-bottom: 0.3125rem;
        margin-right: 1rem;
        font-size: 1.25rem;
        line-height: inherit;
        white-space: nowrap;
    }
</style>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
