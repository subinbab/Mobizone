@model DomainLayer.ProductModel.Master.MasterTable

@{
    ViewData["Title"] = "MasterDelete";
}

<h3>Delete Master Data</h3>
<div>
    <h4>MasterTable</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.masterData)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.masterData)
        </dd>

    </dl>
    <div class="row">
        <div class="modal-footer col">
            <form asp-action="MasterDelete">
                <input asp-for="id" type="hidden" value="@Model.id" />
