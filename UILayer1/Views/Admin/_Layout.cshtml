<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title </title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/SideBar.css" />
    <link rel="stylesheet" href="~/css/navigation-bar.css" />
    <link rel="stylesheet" href="~/css/footer.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css"/>
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Kanit&family=Oswald:wght@500&family=Roboto+Condensed:wght@700&family=Rubik&family=Sansita+Swashed:wght@600&family=Satisfy&display=swap" rel="stylesheet">

</head>
<body>
    <partial name="Admin\_navbar" />
    <partial name="Admin\_sideBarMenu" />
    <div class="content " style="padding-bottom:10px">
        <div class="container-fluid">
            <main role="main" class="pb-3">
                <div id="mypartialview"></div>

                @RenderBody()
            <footer>
                <div class="row justify-content-center"><img src="~/css/Images/depositphotos_69864645-stock-illustration-letter-m-logo-icon-design-removebg-preview-removebg-preview.png" alt=" Mobizone" style="height:60px; width:60px;"
     class="float-left" />
                    
                    <ul class="list">

                        </li>
                        <li class="about">
                            <a asp-controller="Admin" asp-action="Company" style="margin-right:-86px">Company</a>
                        </li>
                        <li class="about">
                            <a asp-controller="Admin" asp-action="About" style="margin-right:-86px">About</a>
                        </li>

                        <li class="about">
                            <a asp-controller="Admin" asp-action="Privacy" style="margin-right:-86px">Privacy & Policy</a>
                        </li>

                        <li class="about">

                            <a asp-controller="Admin" asp-action="Contact" style="margin-right:-86px">Contact Us</a>


                        </li>
                    </ul>

                </div>




                <div style="font-size: 12px; padding-bottom:10px">
                    <img src="~/css/Images/copyright-38672_1280.png" style="height: 10px; width: 10px;" /><a> Mobizone private limited all right reserved</a>

                </div>

            </footer>
            </main>
        </div>
    </div>





   
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/ckeditor/ckeditor.js"></script>
    
    @await Component.InvokeAsync("Notyf")
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

<style>
    .card{
         font-variant: common-ligatures !important;
    }
    li{
        display:inline;
    }

   
