@model DomainLayer.ProductModel.Master.MasterTable

<div class="row">
    <div class="col-md-4">
        <form asp-action="MasterEdit" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
               
                <input asp-for="id" class="form-control" type="hidden"/>
                
            </div>
            <div class="form-group">
                <input asp-for="masterData" class="form-control"  required/>
                <span asp-validation-for="masterData" class="text-danger"></span>
            </div>
            <div class="form-group">
                
                <input asp-for="parantId" class="form-control" type="hidden"/>
                
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
