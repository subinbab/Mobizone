@model DomainLayer.Contact
    <section class=" flex-fill" style="margin-left: 0px;">
        <div class=" text-center">

            <h1>Contact Us</h1>
            <br />
            <br />
            <div class=" " container>
                <div class=" contact us"></div>
                <div class="box"></div>
                <div class="icon"></div>
                <div class=" text">
                    <h3>Address</h3>
                    <p>@Model.shopName<br>@Model.district <br> @Model.state <br>Pin:@Model.pincode</p>
                </div>
                <div class="box"></div>
                <div class="icon"></div>
                <div class=" text">
                    <h3>Email</h3>
                    <p> @Model.email</p>
                </div>
            </div>
            <div class="box"></div>
            <div class="icon"></div>
            <div class=" text">
                <h3>Phone</h3>
                <p>@Model.phoneNumber</p>
            </div>
        </div>

    </section>